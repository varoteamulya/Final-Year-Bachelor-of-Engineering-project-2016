<script>

       var DB_VERSION = "1.0";
    var DB_NAME = "Database";
    var DB_DISPLAY_NAME = "Cordova Demo";
    var DB_SIZE = 20000;
    var DEBUG=true;
    var db=window.openDatabase (DB_NAME, DB_VERSION, DB_DISPLAY_NAME, DB_SIZE);
    var imgdata;


   $( document ).bind( 'mobileinit', function(){
  $.mobile.loader.prototype.options.text = "loading";
  $.mobile.loader.prototype.options.textVisible = false;
  $.mobile.loader.prototype.options.theme = "c";
  $.mobile.loader.prototype.options.html = "";
});



    $("#logout").bind('click', function(){
    var user_type=window.localStorage.getItem("user_type");
     if(user_type=="admin")
        document.location.href="./login.html";
    else
    document.location.href="./login.html";
    });

     //fetch from cloud
     $("#fetchfromcloud").bind('click', function(){
        document.location.href="http://hiwicloud.appspot.com/patientcloud.jsp?id="+window.localStorage.getItem("email");;

    });

     function start() {
       //var db = window.openDatabase("Database", "1.0", "Cordova Demo", 200000);
       db.transaction(search, errorCB, success);
     }

     function errorCB(err) {
      if(DEBUG)
      console.log("Error processing SQL: "+err.code);
     }


     function search(tx) {
	      var id=window.localStorage.getItem("id");
	      //alert("Searching for patient with id "+id);
	      tx.executeSql('SELECT * FROM DETAILS_PATIENT where id='+id, [], querySuccess, errorCB);
     }

    function querySuccess(tx, results) {
      var len = results.rows.length;
      if(len==0)
      alert("You have not signed up!, please sign up!");
      else
      {
      var id;     
      id=results.rows.item(0).id;
      //alert("Patient ID is "+id);
      fn=results.rows.item(0).fname;
      mn=results.rows.item(0).mname;
      ln=results.rows.item(0).lname;
      var gender=results.rows.item(0).gender;
      var dob=results.rows.item(0).dob;
      var pcontactno=results.rows.item(0).pcontactno;
      var scontactno=results.rows.item(0).scontactno;

      $("#name").html(fn+" "+mn+" "+ln);
      $("#gender").html(gender);
      $("#dob").html(dob);
      $("#pcontactno").html(pcontactno);
      $("#scontactno").html(scontactno);

      window.localStorage.setItem("id",id);    
      window.localStorage.setItem("fname",fn);
      window.localStorage.setItem("mname",mn);
      window.localStorage.setItem("lname",ln);
      window.localStorage.setItem("gender",gender);
      window.localStorage.setItem("dob",dob);
      window.localStorage.setItem("pcontactno",pcontactno);
      window.localStorage.setItem("scontactno",scontactno);

     /* var dbImage = document.getElementById('myimage');
      dbImage.style.display = 'block';
      dbImage.src="data:image/jpeg;base64,"+results.rows.item(0).image;
      imgdata="data:image/jpeg;base64,"+results.rows.item(0).image;*/
      }
    }

     function success(){
      // alert("sucess");
     }
      $("#exitButton,#exitButton1,#exitButton0,#exitButton2").bind('click', function(){


              navigator.notification.confirm(
                        'Do you want to quit',
                        onConfirmQuit,
                        'Exit Application',
                        'OK,Cancel'
                    );

      });





    function faddingMessage(e){$.mobile.showPageLoadingMsg("a",e,true);setTimeout($.mobile.hidePageLoadingMsg,2500)}
    function faddingMessage2(e){$.mobile.showPageLoadingMsg("a",e);setTimeout($.mobile.hidePageLoadingMsg,20000)}


    //sensor interfacing

     document.addEventListener("deviceready", onDeviceReady, false);
    function onDeviceReady() {
    //alert("pp");

    //window.localStorage.setItem("first","no");



    start();

    $.mobile.hidePageLoadingMsg();
     document.addEventListener("backbutton", function () {
              navigator.notification.confirm(
                        'Do you want to quit',
                        onConfirmQuit,
                        'Exit Application',
                        'OK,Cancel'
                    );
            }, true);



      }//);




    function onConfirmQuit(button){
       if(button == "1"){


      window.CustomNativeAccess.exitapp();
    }
  }






