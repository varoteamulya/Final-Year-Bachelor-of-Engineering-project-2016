   opacity: 0.5,           // Falls back to a filter for IE.
   cssFilterSupport: true  // Use "-webkit-filter" where available.
 });
      $("#THERMOMETER_FINAL_Id_a").attr("disabled","disabled");

      $("#TransparentHeart2_Id").foggy({
   blurRadius: 5,          // In pixels.
   opacity: 0.5,           // Falls back to a filter for IE.
   cssFilterSupport: true  // Use "-webkit-filter" where available.
 });
      $("#TransparentHeart2_Id_a").attr("disabled","disabled");

      $("#logo_Id").foggy({
   blurRadius: 5,          // In pixels.
   opacity: 0.5,           // Falls back to a filter for IE.
   cssFilterSupport: true  // Use "-webkit-filter" where available.
 });
      $("#logo_Id_a").attr("disabled","disabled");

      $("#ecg1_Id").foggy({
   blurRadius: 5,          // In pixels.
   opacity: 0.5,           // Falls back to a filter for IE.
   cssFilterSupport: true  // Use "-webkit-filter" where available.
 });
      $("#ecg1_Id_a").attr("disabled","disabled");

      $("#Cloud_Id").foggy({
   blurRadius: 5,          // In pixels.
   opacity: 0.5,           // Falls back to a filter for IE.
   cssFilterSupport: true  // Use "-webkit-filter" where available.
 });
      $("#fetchfromcloud").attr("disabled","disabled");


      $("#WeighScaleEdited_Id").foggy({
   blurRadius: 5,          // In pixels.
   opacity: 0.5,           // Falls back to a filter for IE.
   cssFilterSupport: true  // Use "-webkit-filter" where available.
 });
      $("#WeighScaleEdited_Id_a").attr("disabled","disabled");
  }

  function Un_Blur_Page()
  {
      $("#MultiFunction_Button_Id").fadeIn(500);
      $("#Multiple_Extended_Id").hide();

      $("#PULSE_OXIMETER_Id").foggy(false);
      $("#PULSE_OXIMETER_Id_a").removeAttr("disabled");

      $("#bloodPressure_Id").foggy(false);
      $("#bloodPressure_Id_a").removeAttr("disabled");


      $("#THERMOMETER_FINAL_Id").foggy(false);
      $("#THERMOMETER_FINAL_Id_a").removeAttr("disabled");

      $("#TransparentHeart2_Id").foggy(false);
      $("#TransparentHeart2_Id_a").removeAttr("disabled");

      $("#logo_Id").foggy(false);
      $("#logo_Id_a").removeAttr("disabled");

      $("#ecg1_Id").foggy(false);
      $("#ecg1_Id_a").removeAttr("disabled");

      $("#Cloud_Id").foggy(false);
      $("#fetchfromcloud").removeAttr("disabled");

      $("#WeighScaleEdited_Id").foggy(false);
      $("#WeighScaleEdited_Id_a").removeAttr("disabled");
  }

  $(document).ready(function(){

    $("body").click(function()
    {
      if(Blur_flag == 0)
      Un_Blur_Page();
      else
      {
        Blur_flag = 0;
      }
    });

    $("#MultiFunction_Button_Id").click(function(){
      Blur_flag = 1;
      Blur_Page();
    });

    $("#Multiple_Extended_Id").click(function(){
      Blur_flag = 1;
    });
  });


