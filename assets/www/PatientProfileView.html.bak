  <script>
  //PRATEEK
  //Functions to Blur and unblur the User interface when multifunction Button is clicked

  var Blur_flag = 0;

  function Blur_Page()
  {
      $("#MultiFunction_Button_Id").hide();
      $("#Multiple_Extended_Id").fadeIn(500);

      $("#PULSE_OXIMETER_Id").foggy({
   blurRadius: 5,          // In pixels.
   opacity: 0.5,           // Falls back to a filter for IE.
   cssFilterSupport: true  // Use "-webkit-filter" where available.
 });
      $("#PULSE_OXIMETER_Id_a").attr("disabled","disabled");

      $("#bloodPressure_Id").foggy({
